    <!-- CLIENTS -->
    <section class="module module-parallax bg-light-60" data-background="/images/section-3.jpg">

        <div class="container">

            <div class="row">

                <!-- Owl-carousel start -->
                <div class="slider-clients owl-carousel text-center">
                    {{#each works}}
