    <!-- HERO -->
    <section id="hero" class="module-hero module-parallax module-full-height bg-film" >

        <!-- HERO TEXT -->
        <div class="hero-caption">
            <div class="hero-text">
                <h5 class="mh-line-size-4 font-alt m-b-50">Simple &amp; Powerful</h5>
