
<!-- PRELOADER -->
<div class="page-loader">
    <div class="loader">Loading...</div>
</div>
<!-- /PRELOADER -->

<!-- WRAPPER -->
<div class="wrapper">

    <!-- NAVIGATION -->
    {{>nav}}
    <!-- /NAVIGATION -->



    <!-- LATEST PRODUCTS -->
    <section id="shop" class="module-small">
        <div class="container">


            <div class="row multi-columns-row">
                {{#each works}}
                <!-- SHOP ITEM -->
                <div class="col-sm-6 col-md-3 col-lg-3">
                    <div class="shop-item">
                        <div class="shop-item-image">
                            <a href="{{link}}"><img src="/uploads/{{imgPath}}" alt=""></a>
                            <div class="shop-item-detail">
                                <a class="btn btn-round btn-b" href="{{link}}">{{title}}<br/>{{desc}}</a>
                            </div>
                        </div>
                    </div>
                </div>
                {{/each}}
                <!-- /SHOP ITEM -->

            </div>

            <div class="row m-t-10">
                <div class="col-sm-12 text-center">
                    <nav>
                        {{{paginateHelper pagination paginationClass='pagination font-alt' leftText='<span aria-hidden="true">&laquo;</span><span class="sr-only">Previous</span>' rightText='<span aria-hidden="true">&raquo;</span><span class="sr-only">Next</span>'}}}
                    </nav>

                </div>
            </div>

        </div>

    </section>
    <!-- /LATEST PRODUCTS -->
    {{>footer}}
</div>
<!-- /WRAPPER -->

